{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Active CSS",
	"patterns": [
		{
			"include": "#at_rules"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"at_rules": {
			"patterns": [{
				"name": "keyword.control.activecss",
				"match": "\\s*((@)(if|while|for|each|else|command|component)\\b)\\s*"
			}]
		},
		"keywords": {
			"patterns": [{
				"name": "keyword.other.activecss",
				"match": "^\\s*(add\\-class|add\\-hash|ajax\\-form\\-preview|ajax\\-form\\-submit|ajax\\-pre\\-get|ajax|alert|blur|cancel\\-timer\\-all|cancel\\-timer|click\\-on\\-first|click\\-on\\-last|click\\-on\\-next\\-cycle|click\\-on\\-next|click\\-on\\-previous\\-cycle|click\\-on\\-previous|clickoutside\\-event|clone|console\\-log|copy\\-to\\-clipboard|create\\-command|create\\-conditional|create\\-element|document\\-title|eval|fade\\-in|fade\\-out|fade\\-to|focus\\-on\\-first|focus\\-on\\-last|focus\\-on\\-next\\-cycle|focus\\-on\\-next|focus\\-on\\-previous\\-cycle|focus\\-on\\-previous|focus\\-on|form\\-reset|fullscreen\\-exit|fullscreen\\-on|fullscreen|func|iframe\\-reload|load\\-as\\-ajax|load\\-config|load\\-images|load\\-script|load\\-style|location|media\\-control|mimic\\-into|pause|prevent\\-default|print|remove\\-attribute|remove\\-class|remove\\-clone|remove\\-cookie|remove\\-hash|remove\\-property|remove|render\\-after\\-begin|render\\-after\\-end|render\\-before\\-begin|render\\-before\\-end|render\\-replace|render|restore\\-clone|run|scroll\\-into\\-view|scroll\\-x|scroll\\-y|set\\-attribute|set\\-class|set\\-cookie|set\\-property|stop\\-event\\-propagation|stop\\-immediate\\-event\\-propagation|stop\\-immediate\\-propagation|stop\\-propagation|style|take\\-class|toggle\\-class|trigger\\-real|trigger|url\\-change|var\\-delete|var)(?=[\\s]*\\:)"
			}]
		},
		"strings": {
			"name": "string.quoted.double.activecss",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.activecss",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.acss"
}